	key
	{
		modkey = {"Mod4"}
		key = "q"
		command = "spawn"
		arg = "`dmenu_path | dmenu -nb '#000000' -nf '#ffffff'` && exec $exe"
	}
	key
	{
		key = "Print"
		command = "spawn"
		arg = "exec scrot 'screen-%Y-%m-%d.png' -e 'mv $f ~/Pictures/screens/'"
	}
     key
